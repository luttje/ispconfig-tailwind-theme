<input type="{{ type }}"
    name="{{ name }}"
    id="{{ id }}"
    value="{{ value }}"
    class="w-full h-full border rounded px-2 py-1 {{ class }} bg-white text-black border-gray-400 dark:bg-slate-600 dark:text-white dark:border-slate-700"
    autocomplete="{{ autocomplete }}"
    placeholder="{{ placeholder }}"
    onkeyup="{{ onkeyup }}" />
